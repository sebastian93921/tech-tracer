{
  "name": "tech-tracer",
  "version": "1.0.0",
  "description": "An application for monitoring and analyzing network traffic",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "start:url": "electron . --url",
    "build": "electron-builder",
    "test": "mocha tests/**/*.test.js",
    "test:unit": "mocha tests/unit/**/*.test.js",
    "test:integration": "mocha tests/integration/**/*.test.js"
  },
  "keywords": [
    "chromium"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.7",
    "electron": "^35.1.5",
    "electron-builder": "^24.9.1",
    "mocha": "^10.2.0",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.1"
  },
  "dependencies": {
    "electron-store": "^8.2.0"
  },
  "build": {
    "appId": "com.TechTracer.app",
    "productName": "TechTracer",
    "directories": {
      "output": "dist"
    },
    "icon": "build/icons/icon.png",
    "mac": {
      "icon": "build/icons/icon.png",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "icon": "build/icons/icon.png",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "icon": "build/icons/icon.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
